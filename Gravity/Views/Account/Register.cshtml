<body>
    <!--[if lt IE 8]>
        <p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
    <![endif]-->
    <div class="auth-wrapper">
        <div class="container-fluid h-100">
            <div class="row flex-row h-100 bg-white">
                <div class="col-xl-8 col-lg-6 col-md-5 p-0 d-md-block d-lg-block d-sm-none d-none">
                    <div class="lavalite-bg" style="background-image: url('/demo/img/auth/register-bg.jpg')">
                        <div class="lavalite-overlay"></div>
                    </div>
                </div>
                <div class="col-xl-4 col-lg-6 col-md-7 my-auto p-0">
                    <div class="authentication-form mx-auto">
                        <div class="logo-centered">
                            <a href="/"><img src="/images/icon/logo.png" alt=""></a>
                        </div>
                        
                        <p>Join us today! It takes only few steps</p>
                        @using (Html.BeginForm())
                        {
                            <div style="color:red">@TempData["msg"]</div>
                            <div class="form-group">
                                <input name="email" type="email" class="form-control" placeholder="Email" required="">
                                <i class="ik ik-user"></i>
                            </div>
                            <div class="form-group">
                                <input id="password" name="password" type="password" class="form-control" placeholder="Password" required="">
                                <i class="ik ik-lock"></i>
                            </div>
                            <div class="form-group">
                                <input id="confirmpassword" name="confirmpassword" type="password" class="form-control" placeholder="Confirm Password" required="">
                                <i class="ik ik-eye-off"></i>
                            </div>
                            <div class="row">
                                <div class="col-12 text-left">
                                    <label class="custom-control custom-checkbox">
                                        <input required type="checkbox" class="custom-control-input" id="aggree" name="aggree" value="">
                                        <span class="custom-control-label">&nbsp;I Accept <a href="#">Terms and Conditions</a></span>
                                    </label>
                                </div>
                            </div>
                            <div class="sign-btn text-center">
                                <button class="btn btn-theme" type="submit" id="btnRegister">Create Account</button>
                            </div>
                        }
                        <div class="register">
                            <p>Already have an account? <a href="/Account/Login">Sign In</a></p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <script src="~/demo/code.jquery.com/jquery-3.3.1.min.js"></script>
    <script>window.jQuery || document.write('<script src="~/demo/src/js/vendor/jquery-3.3.1.min.js"><\/script>')</script>
    <script src="~/demo/node_modules/popper.js/dist/umd/popper.min.js"></script>
    <script src="~/demo/node_modules/bootstrap/dist/js/bootstrap.min.js"></script>
    <script src="~/demo/node_modules/perfect-scrollbar/dist/perfect-scrollbar.min.js"></script>
    <script src="~/demo/node_modules/screenfull/dist/screenfull.js"></script>
    <script src="~/demo/dist/js/theme.js"></script>
    <script src="~/lib/jquery-validation/dist/jquery.validate.js"></script>
    <script>
        $(function () {

            $.validator.addMethod("RequireDigit", function(value, element) {
                return /([0-9])/.test(value);
            }, " Requires a number between 0-9 in the password.");

            $.validator.addMethod("RequireLowercase", function(value, element) {
                return /[a-z]/.test(value);
            }, " Requires a lowercase character in the password.");

            $.validator.addMethod("RequireNonAlphanumeric", function(value, element) {
                return /[^a-zA-Z 0-9]+/g.test(value);
            }, " Requires a non-alphanumeric character in the password.");

            $.validator.addMethod("RequiredUniqueChars", function(value, element) {
                return /([~,!,@@,#,$,%,^,&,*,-,_,+,=,?,>,<])/.test(value);
            }, " Requires the number of special characters in the password.");

            $.validator.addMethod("RequireUppercase", function(value, element) {
                return /[A-Z]/.test(value);
           }, " Requires an uppercase character in the password.");


            $("form:last").validate({
                rules: {
                    password: {
                        minlength: 6,//RequiredLength
                        RequireLowercase: true,
                        RequireDigit: true,
                        RequiredUniqueChars: true,
                        RequireUppercase: true,
                        RequireNonAlphanumeric: true
                        
                    },
                    confirmpassword: {
                        equalTo: "#password"
                    }
                }
            });
        })
    </script>

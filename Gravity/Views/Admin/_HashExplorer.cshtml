@model IEnumerable<MineTransaction>


    <div class="col-sm-12">
        <div class="card">
            <div class="card-header d-block">
                <h3>Transactions</h3>
            </div>
            <div class="card-body">
                <div class="dt-responsive">
                    <table id="table" class="table table-responsive">
                        <thead>
                            <tr>
                                <th>Height</th>
                                <th>Date</th>
                                <th>Hash</th>
                                <th>Size</th>

                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                string url = "https://rinkeby.etherscan.io/tx/" + item.txHash;
                                <tr>
                                    <td>573116 (Main Chain)</td>
                                    <td>@item.CreationDate.ToLocalTime()</td>
                                    <td><a href="/Admin/Hash?hash=@item.txHash"> @item.txHash </a></td>
                                    <td class="text-right">1,324.5</td>

                                </tr>
                            }
                            



                        </tbody>

                    </table>
                </div>
            </div>
        </div>


        <!-- Language - -->
    </div>

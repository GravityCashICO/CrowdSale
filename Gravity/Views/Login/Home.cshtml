    <div class="container-fluid">
        <div class="page-header">

            <div class="row align-items-end">
                <div class="col-lg-8">
                    <div class="page-header-title">
                        <i class="ik ik-file-text bg-dark"></i>
                        <div class="d-inline">
                            <h5> Transactions (GCH)</h5>
                            <span>Gravity Cash Transactions History</span>
                        </div>
                    </div>
                </div>
                <div class="col-lg-4">
                    <nav class="breadcrumb-container" aria-label="breadcrumb">
                        <ol class="breadcrumb">

                            <li class="breadcrumb-item">

                            </li>

                        </ol>
                    </nav>
                </div>
            </div>

        </div>


        <!--Account Balance --><div class="row clearfix">
            <div class="col-lg-6 col-md-6 col-sm-12">
                <div class="widget bg-dark">
                    <div class="widget-body">
                        <div class="d-flex justify-content-between align-items-center">
                            <div class="state">
                                <h6>Account Balance</h6>
                                <partial name="TotalBalance"/>

                            </div>
                            <div class="icon">
                                <i class="ik ik-file-text"></i>
                            </div>
                        </div>
                    </div>
                </div>
            </div>


            <div class="col-lg-6 col-md-6 col-sm-12">
                <a href="/Login/ICO">
                    <div class="widget bg-default">


                        <div class="widget-body">



                            <div class="d-flex justify-content-between align-items-center">

                                <div class="state">

                                    <h2 style="color: black;">Buy GCH (ICO)</h2>
                                    <span style="color:darkviolet;">GCH @@ @Admin.CoinPriceUSD USD</span>
                                </div>

                                <div class="icon">
                                    <img src="~/demo/img/logo/gch-black-logo.png" style="max-width: 50px;">
                                </div>

                            </div>
                        </div>
                    </div>

                </a>
            </div>



        </div>

        <!--Account Balance End -->









        <div class="row">
            <div class="col-sm-12">
                <div class="card">
                    <div class="card-header d-block">
                        <h3>Recent Transactions</h3>
                    </div>
                    
                </div>

                <partial name="_Transactions" model="Model">
                
                <!-- Language - -->
            </div>
        </div>
    </div>


    